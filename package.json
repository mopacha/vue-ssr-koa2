{
  "name": "vue-ssr-koa2",
  "version": "1.0.0",
  "description": "based on vue2 koa2 webpack4 for ssr render",
  "scripts": {
		"dev": "node server/index.js",
    "start": "npm run build && cross-env NODE_ENV=production npm run dev",
    "fix-eslint": "eslint client --ext .js,.vue --fix",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config config/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config config/webpack.server.config.js --progress --hide-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mopacha/vue-ssr-koa2.git"
  },
  "keywords": [
    "vue2",
    "vue-ssr",
    "ssr",
    "koa",
    "koa2",
    "webpack4"
  ],
  "author": "moofyu@163.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mopacha/vue-ssr-koa2/issues"
  },
  "homepage": "https://github.com/mopacha/vue-ssr-koa2#readme",
  "dependencies": {
    "koa": "^2.8.2",
    "koa-better-http-proxy": "^0.2.4",
    "koa-compress": "^3.0.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vue-server-renderer": "^2.6.10",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.6.0",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "ip": "^1.1.5",
    "json-loader": "^0.5.7",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "^2.1.0",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "webpack": "4.25.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-middleware": "3.4.0",
    "webpack-hot-middleware": "2.24.3",
    "webpack-merge": "4.1.4",
    "webpack-node-externals": "1.7.2"
  }
}
